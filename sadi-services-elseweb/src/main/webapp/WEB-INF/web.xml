<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

  <servlet>
    <servlet-name>LifemapperService</servlet-name>
    <servlet-class>edu.utep.cybershare.elseweb.service.lifemapper.LifemapperService</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WCSPayloadExtractorService</servlet-name>
    <servlet-class>edu.utep.cybershare.elseweb.service.adaptor.WCSPayloadExtractorService</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>WCSCoverageDistributionService</servlet-name>
    <servlet-class>edu.utep.cybershare.elseweb.service.mapper.WCSCoverageDistributionService</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ScenarioMapperService</servlet-name>
    <servlet-class>edu.utep.cybershare.elseweb.service.mapper.ScenarioMapperService</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>LifemapperScenarioMapperService</servlet-name>
    <servlet-class>edu.utep.cybershare.elseweb.service.adaptor.LifemapperScenarioMapperService</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>LifemapperService</servlet-name>
    <url-pattern>/LifemapperService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LifemapperScenarioMapperService</servlet-name>
    <url-pattern>/LifemapperScenarioMapperService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ScenarioMapperService</servlet-name>
    <url-pattern>/ScenarioMapperService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WCSPayloadExtractorService</servlet-name>
    <url-pattern>/WCSPayloadExtractorService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WCSCoverageDistributionService</servlet-name>
    <url-pattern>/WCSCoverageDistributionService</url-pattern>
  </servlet-mapping>
  
</web-app>