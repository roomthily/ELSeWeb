PREFIX edac: <https://raw.github.com/nicholasdelrio/ELSeWeb/master/documents/semantic-web/rdf/ontology/edac-v3.owl#>
prefix lifemapper: <https://raw.github.com/nicholasdelrio/ELSeWeb/master/documents/semantic-web/rdf/ontology/lifemapper-v3.owl#>
prefix experiment: <https://raw.github.com/nicholasdelrio/ELSeWeb/master/documents/semantic-web/rdf/ontology/experiments/experiment-1-v3.owl#>
prefix owl: <http://www.w3.org/2002/07/owl#>
select ?algorithmName ?units ?occurrenceSetID ?modelURL
FROM <https://raw.github.com/nicholasdelrio/ELSeWeb/master/documents/semantic-web/rdf/ontology/experiments/experiment-1-v3.owl>
FROM <https://raw.github.com/nicholasdelrio/ELSeWeb/master/documents/semantic-web/rdf/ontology/edac-data.owl>
where
{
?experiment lifemapper:hasModelingAlgorithm ?algorithm .
?algorithm lifemapper:hasName ?algorithmName .
?experiment lifemapper:hasScenarioLayerUnits ?units .
?experiment lifemapper:hasOccurrenceSetID ?occurrenceSetID .
?experiment lifemapper:hasLifemapperModelURL ?modelURL .
?experiment a lifemapper:Experiment_Stage4 .
}